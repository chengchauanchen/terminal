<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent" />
            <stroke android:color="@color/button_shape_stoke_blue_color"
                android:width="@dimen/button_shape_stoke_width"/>
            <corners android:radius="@dimen/select_member_bg_round_size"/>

        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent" />
            <stroke android:color="@color/regist_botton_press_color"
                android:width="@dimen/button_shape_stoke_width"/>
            <corners android:radius="@dimen/select_member_bg_round_size"/>
        </shape>
    </item>
</selector>
